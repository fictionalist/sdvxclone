{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "Make: build project",
			"command": "mingw32-make",
			"args": [
				"all"
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"group": "build"
		},
		{
			"type": "cppbuild",
			"label": "Make: clean",
			"command": "mingw32-make",
			"args": [
				"clean"
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"group": "none"
		}
	]
}